    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testRegex": [
      "test/.+\\.(test|spec)\\.(jsx?|tsx?)$"
    ]
  },
  "dependencies": {
    "dayjs": "^1.9.1"
